{  
    "extends": [
        "plugin:prettier/recommended"
      ],
    
      "plugins": ["prettier"],
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    }, 
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",    
        "linebreak-style": 0,
        "prettier/prettier": "error"        
    }
}       
